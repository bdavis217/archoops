{
  "name": "@archoops/api",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env DATABASE_URL=file:./prisma/dev.db JWT_SECRET=dev-secret-key NODE_ENV=development PORT=3001 tsx watch src/index.ts",
    "dev:env": "cross-env DATABASE_URL=file:./prisma/dev.db JWT_SECRET=dev-secret-key NODE_ENV=development PORT=3001 tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@archoops/config": "workspace:*",
    "@archoops/types": "workspace:*",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "7",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/sensible": "^5.6.0",
    "@fastify/static": "6",
    "@prisma/client": "5.22.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "cross-env": "^10.0.0",
    "prisma": "5.22.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
